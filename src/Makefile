### Do not edit -- Generated by 'configure --with-whatever' from Makefile.in
### RtMidi tests Makefile - for various flavors of unix

PROGRAMS = project    
RM = /bin/rm
SRC_PATH = ..
INCLUDE = ..
OBJECT_PATH = Release
vpath %.o $(OBJECT_PATH)


CC       = g++
DEFS     = -std=c++0x
CFLAGS   = -O3 -Wall -Wextra
LIBRARY  = -pthread 


%.o : $(SRC_PATH)/%.cpp
	$(CC) $(CFLAGS) $(DEFS) -c $(<) -o $(OBJECT_PATH)/$@

all : $(PROGRAMS)

project : version6.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o project version6.cpp $(LIBRARY)

clean : 
	$(RM) -f $(OBJECT_PATH)/*.o
	$(RM) -f $(PROGRAMS) *.exe
	$(RM) -f *~
	$(RM) -fR *.dSYM
	$(RM) -f ch*.csv
